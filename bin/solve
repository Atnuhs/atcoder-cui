#!/bin/bash
trap 'echo "ERROR: line no = $LINENO, exit status = $?" >&2; exit 1' ERR

ROOT="$(cd $(dirname $0)/.. && pwd)"
source "${ROOT}/bin/.env"

DIRNAME_DST="${ROOT}/solve"
DIRNAME_SRC="${ROOT}/go"

[ -d "${DIRNAME_DST}" ] || mkdir "${DIRNAME_DST}"
cp "${DIRNAME_SRC}/main.go" "${DIRNAME_DST}/main.go"
